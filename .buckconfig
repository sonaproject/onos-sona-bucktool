[buildfile]
  includes = //stable-onos/buck-tools/default.defs

[plugins]
  directory = //bin/plugins

[java]
  source_level = 8
  target_level = 8

[alias]
    onos = //stable-onos/tools/package:onos-package
    onos-local = //stable-onos/tools/package:onos-run
    buck-plugin = //stable-onos/tools/build/buck-plugin:onos

[download]
  in_build = true

[maven_repositories]
  central = https://repo1.maven.org/maven2

[project]
  ide = intellij
  ignore = .git, bazel-bin, bazel-genfiles, bazel-onos-next, bazel-out, bazel-testlogs
